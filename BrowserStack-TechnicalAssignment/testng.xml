<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="ParallelTestSuite" parallel="tests" thread-count="5">
    <test name="Chrome on Windows">
        <parameter name="browser" value="Chrome"/>
        <parameter name="os" value="Windows"/>
        <parameter name="osVersion" value="10"/>
        <classes>
            <class name="org.com.BrowserStackAssignment.ElPaisScraper1"/>
        </classes>
    </test>
    <test name="Firefox on Windows">
        <parameter name="browser" value="Firefox"/>
        <parameter name="os" value="Windows"/>
        <parameter name="osVersion" value="11"/>
        <classes>
            <class name="org.com.BrowserStackAssignment.ElPaisScraper1"/>
        </classes>
    </test>
    <test name="Safari on iPhone">
        <parameter name="device" value="iPhone 14"/>
        <parameter name="osVersion" value="16"/>
        <classes>
            <class name="org.com.BrowserStackAssignment.ElPaisScraper1"/>
        </classes>
    </test>
    <test name="Chrome on Android">
        <parameter name="device" value="Samsung Galaxy S22"/>
        <parameter name="osVersion" value="12.0"/>
        <classes>
            <class name="org.com.BrowserStackAssignment.ElPaisScraper1"/>
        </classes>
    </test>
    <test name="Edge on Windows">
        <parameter name="browser" value="Edge"/>
        <parameter name="os" value="Windows"/>
        <parameter name="osVersion" value="11"/>
        <classes>
            <class name="org.com.BrowserStackAssignment.ElPaisScraper1"/>
        </classes>
    </test>
</suite>